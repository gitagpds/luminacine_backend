    <h3 style="font-size: 24px; text-align: center;">FINAL PROJECT PRAKTIKUM TEKNOLOGI CLOUD COMPUTING</h3>
    <h3 style="font-size: 24px; text-align: center; margin-bottom: 20px;">DAFTAR ENDPOINT API</h3>
    <table border="1" style="margin: auto;">
      <thead style="text-align: center;">
        <tr>
          <td>Metode</td>
          <td>Endpoint</td>
          <td>Deskripsi</td>
          <td>Request Body</td>
          <td>Contoh Response</td>
          <td>Tabel Database</td>
        </tr>
      </thead>
      <tbody style="font-family: monospace">
        <tr>
          <td>GET</td>
          <td>/users</td>
          <td>Mengambil seluruh data dari tabel users.</td>
          <td>-</td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Users Retrieved", <br />
            &nbsp; data: [ <br />
            &nbsp;&nbsp; { <br />
            &nbsp;&nbsp;&nbsp; "id": 1, <br />
            &nbsp;&nbsp;&nbsp; "name": "rafli", <br />
            &nbsp;&nbsp;&nbsp; "email": "rafli@gmail.com", <br />
            &nbsp;&nbsp;&nbsp; "gender": "Male", <br />
            &nbsp;&nbsp;&nbsp; "createdAt": "2025-04-29T13:17:17.000Z", <br />
            &nbsp;&nbsp;&nbsp; "updatedAt": "2025-04-29T13:17:17.000Z" <br />
            &nbsp;&nbsp; }, <br />
            &nbsp;&nbsp; ... <br />
            &nbsp; ] <br />
            }
          </td>
          <td>User</td>
        </tr>
        <tr>
          <td>GET</td>
          <td>/users/:id</td>
          <td>Mengambil data dari tabel users berdasarkan id.</td>
          <td>-</td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Users Retrieved", <br />
            &nbsp; data: { <br />
            &nbsp;&nbsp; "id": 1, <br />
            &nbsp;&nbsp; "name": "rafli", <br />
            &nbsp;&nbsp; "email": "rafli@gmail.com", <br />
            &nbsp;&nbsp; "gender": "Male", <br />
            &nbsp;&nbsp; "createdAt": "2025-04-29T13:17:17.000Z", <br />
            &nbsp;&nbsp; "updatedAt": "2025-04-29T13:17:17.000Z" <br />
            &nbsp; } <br />
            }
          </td>
          <td>User</td>
        </tr>
        <tr>
          <td>POST</td>
          <td>/users</td>
          <td>Menambahkan data baru ke tabel users.</td>
          <td rowspan="2">
            Content-type: application/json<br />
            Contoh isi request body: <br />
            { <br />
            &nbsp; name: "awgnc" <br />
            &nbsp; email: "awg@awg.inc" <br />
            &nbsp; gender: "l" <br />
            }
          </td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Users Created" <br />
            }
          </td>
          <td>User</td>
        </tr>
        <tr>
          <td>PUT</td>
          <td>/users/:id</td>
          <td>Mengubah data dari tabel users berdasarkan id.</td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Users Updated" <br />
            }
          </td>
          <td>User</td>
        </tr>
        <tr>
          <td>DELETE</td>
          <td>/users/:id</td>
          <td>Menghapus data dari tabel users berdasarkan id.</td>
          <td>-</td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Users Deleted" <br />
            }
          </td>
          <td>User</td>
        </tr>
        <!-- Tambahan baris lainnya nanti disesuaikan -->
        <!-- Contoh jika kamu ingin menambah Booking -->
        <tr>
          <td>POST</td>
          <td>/bookings</td>
          <td>Menambahkan data pemesanan baru.</td>
          <td>
            Content-type: application/json<br />
            { <br />
            &nbsp; id_user: 1, <br />
            &nbsp; id_schedule: 2, <br />
            &nbsp; total_price: 50000 <br />
            }
          </td>
          <td>
            { <br />
            &nbsp; status: "Success", <br />
            &nbsp; message: "Booking Created", <br />
            &nbsp; data: { ... } <br />
            }
          </td>
          <td>Booking</td>
        </tr>
      </tbody>
    </table>

